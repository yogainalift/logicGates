    }

    if (enabled){
        if (image_angle &gt;0){
           image_angle -=10;
           x-=1;
        }
    }
    else{
        if (image_angle &lt; 90){
           image_angle +=10 ;
           x+=1;
        }
    }
    if (global.completecheck){
       //step
       depth= -1000;
       if (instance_number(obj_robot)&gt;1){
          gspeed = 1;
       }else{
          gspeed = 2;
       }
       move_to_center(instance_find(obj_robot,0));
       with (instance_find(obj_robot,0)){
           if (!destroyAnimation){
               sprite_index = spr_robot_destroy;
               if (image_xscale &lt; 4){
                  image_xscale +=0.1*gspeed;
                  image_yscale +=0.1*gspeed;
                  sprite_index = spr_robot_walk;
               }
               
               if (sprite_index == spr_robot_destroy){
                  show_debug_message(i);
                  i += 1*gspeed;
                  image_speed +=0.005*gspeed;
                  if (i &gt;=100){
                     image_speed = 1*gspeed;
                     sprite_index = spr_robot_destroy2;
                     destroyAnimation = !destroyAnimation;
                  }
               }
           }
           else{
                if (image_index &gt;= image_number - 1){
                   image_speed = 0;
                }
           }
       }
    }
}

            <string>if (global.pause) exit;
if (instance_exists(obj_output) &amp;&amp; checkNow){
    if (global.count_of_enabled == instance_number(obj_output)){
        global.completecheck=true;
        //global.iscompletesound=false;
        if (!audio_is_playing(bgm_chime) &amp;&amp; chimeenabled==true){
            audio_play_sound(bgm_chime,1,false);
            depth=-1000;
            chimeenabled=false;
            global.scoreon=false;
        }
    }
    if ((keyboard_check_pressed(vk_space) || mouse_check_button_pressed(mb_left))&amp;&amp; global.completecheck==true &amp;&amp; donewait==true){
        global.completecheck=false;
        global.count_of_enabled = 0;
        chimeenabled = true;    
        score=20000; //reset score
        promotiontext = promotion[random(array_length_1d(promotion))]; //gets a random promotional text
        global.scoreon=true;
        donewait=true;
        waittime=0;
        audio_play_sound(bgm_button,1,false);
        room_goto_next();    
    }
}

